	['CreateScript'] = function(Name, Title, Description, ScriptSection, Function, FeaturedBool)

		if FeaturedBool == nil then
			FeaturedBool = false
		end



		if Name == "ButtonTest" then
			return warn("This name is reserved!")
		end

		if not ScriptSection:IsA('Folder') then
			return warn('Error! Script Section is invalid, you must enter a valid instance Folder!')
		end

		local PageSec = ScriptSection.PageSec
		local SecondaryButton = ScriptSection.SecondaryButton

		local ButtonTest_2 = Instance.new("TextButton")

		ButtonTest_2.Name = Name
		ButtonTest_2.Parent = SecondaryButton
		ButtonTest_2.BackgroundColor3 = Color3.fromRGB(29, 30, 30)
		ButtonTest_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ButtonTest_2.BorderSizePixel = 0
		ButtonTest_2.Size = UDim2.new(1, 0, 0, 25)
		ButtonTest_2.Font = Enum.Font.SourceSans
		ButtonTest_2.Text = Title
		ButtonTest_2.TextColor3 = Color3.fromRGB(255, 255, 255)
		ButtonTest_2.TextScaled = true
		ButtonTest_2.TextSize = 14.000
		ButtonTest_2.TextWrapped = true

		ButtonTest_2.MouseButton1Down:Connect(function()
			PageSec.Visible = true

			PageSec.Title.Text = Title
			PageSec.Desc.Text = Description

			PageSec.Title.Visible = true
			PageSec.Desc.Visible = true
			PageSec.Execute.Visible = true

			local Cool = false
			local RBXConnection
			RBXConnection = PageSec.Execute.MouseButton1Down:Connect(function()
				if PageSec.Title.Text == Title then
					local suc, err = pcall(function()
						if not Cool then
							Cool = true
							Function()
							wait(0.01)
							Cool = false
						end
					end)
					if err then
						warn("an error has occured : "..err)
					end
				else
					RBXConnection:Disconnect()
				end
			end)
		end)


		if FeaturedBool then
			local Cool2 = false

			local ButtonTest = Instance.new("TextButton")

			ButtonTest.Name = Name
			ButtonTest.Parent = QuickExecuteScripts
			ButtonTest.BackgroundColor3 = Color3.fromRGB(29, 30, 30)
			ButtonTest.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ButtonTest.BorderSizePixel = 0
			ButtonTest.Size = UDim2.new(1, 0, 0, 25)
