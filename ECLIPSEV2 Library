	['ChangeButtonTitleInMisc'] = function(InstanceButtonName,Sections, Name)
		
		if not MiscSection.Background2:FindFirstChild(Sections) then
			return warn('Cannot find the section you requested in MiscSection')
		end

		if Sections == "Credits" then
			return warn("That slot is reserved!")
