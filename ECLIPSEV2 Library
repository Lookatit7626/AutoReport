-- Scripts:

local function EJMWI_fake_script() -- Profile.LocalScript 
	local script = Instance.new('LocalScript', Profile)

	local Players = game:GetService("Players")

	local player = Players.LocalPlayer

	-- Fetch the thumbnail
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)

	-- Set the ImageLabel's content to the user thumbnail
	local imageLabel = script.Parent
	imageLabel.Image = content
end
coroutine.wrap(EJMWI_fake_script)()
local function YCKZI_fake_script() -- PlayerName.LocalScript 
	local script = Instance.new('LocalScript', PlayerName)


	script.Parent.Text = game.Players.LocalPlayer.Name
end
coroutine.wrap(YCKZI_fake_script)()
local function SIAKUGT_fake_script() -- PlayerLocation.LocalScript 
	local script = Instance.new('LocalScript', PlayerLocation)

	local LocalizationService = game:GetService("LocalizationService")
	local player = game.Players.LocalPlayer

	local result, code = pcall(function()
		return LocalizationService:GetCountryRegionForPlayerAsync(player)
	end)

	local CODE = "US"

	if result then
		CODE = code
	end

	script.Parent.Text = "User based in: "..CODE
end
coroutine.wrap(SIAKUGT_fake_script)()
local function GMNPR_fake_script() -- Player.LocalScript 
	local script = Instance.new('LocalScript', Player)

	local MISCSECTION = script.Parent.Parent.Parent.Background2

	script.Parent.MouseButton1Down:Connect(function()
		local GC = MISCSECTION:GetChildren()
		for i = 1,#GC do
			if GC[i].Name == script.Parent.Name then
				GC[i].Visible = true
			else
				if GC[i].Name ~= "Buttons" then
					GC[i].Visible = false
				end
			end
		end
	end)
end
coroutine.wrap(GMNPR_fake_script)()
local function ZKEYZWZ_fake_script() -- Credits.LocalScript 
	local script = Instance.new('LocalScript', Credits)

	local MISCSECTION = script.Parent.Parent.Parent.Background2

	script.Parent.MouseButton1Down:Connect(function()
		local GC = MISCSECTION:GetChildren()
		for i = 1,#GC do
			if GC[i].Name == script.Parent.Name then
				GC[i].Visible = true
			else
				if GC[i].Name ~= "Buttons" then
					GC[i].Visible = false
				end
			end
		end
	end)
end
coroutine.wrap(ZKEYZWZ_fake_script)()
