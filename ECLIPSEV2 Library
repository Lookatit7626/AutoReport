			Title_2.Position = UDim2.new(0.0500000007, 0, 0.100000001, 0)
			Title_2.Size = UDim2.new(0.699999988, 0, 0.200000003, 0)
			Title_2.Font = Enum.Font.SourceSans
			Title_2.Text = TitleStr
			Title_2.TextColor3 = Color3.fromRGB(255, 255, 255)
			Title_2.TextScaled = true
			Title_2.TextSize = 14.000
			Title_2.TextWrapped = true
			Title_2.TextXAlignment = Enum.TextXAlignment.Left
			Title_2.TextYAlignment = Enum.TextYAlignment.Top

			Image.Name = "Image"
			Image.Parent = Notification_2
			Image.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Image.BackgroundTransparency = 1.000
			Image.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Image.BorderSizePixel = 0
			Image.Position = UDim2.new(0.85, 0, 0.100000001, 0)
			Image.Size = UDim2.new(0.100000001, 0, 0.100000001, 0)
			Image.SizeConstraint = Enum.SizeConstraint.RelativeXX
			Image.Image = ImageStr

			UICorner_6.Parent = Notification_2
			local Happening = false

			Notification_2.MouseButton1Down:Connect(function()
				if not Happening then
					Happening = true

					local goal = {}
					goal.Transparency = 1

					local goal2 = {}
					goal2.ImageTransparency = 1
					
					local goal3 = {}
					goal3.TextTransparency = 1

					local tweenInfo = TweenInfo.new(1)

					local tween = TweenService:Create(Notification_2, tweenInfo, goal)
					tween:Play()

					local tween2 = TweenService:Create(Image, tweenInfo, goal2)
					tween2:Play()
					
					local tween3 = TweenService:Create(Title_2, tweenInfo, goal3)
					tween3:Play()
					
					local tween4 = TweenService:Create(Body, tweenInfo, goal3)
					tween4:Play()

					tween.Completed:Wait()
					Notification_2:Destroy()
				end
			end)

			wait(3)
			if not Happening then
				Happening = true

				local goal = {}
				goal.Transparency = 1

				local goal2 = {}
				goal2.ImageTransparency = 1

				local goal3 = {}
				goal3.TextTransparency = 1

				local tweenInfo = TweenInfo.new(1)

				local tween = TweenService:Create(Notification_2, tweenInfo, goal)
				tween:Play()

				local tween2 = TweenService:Create(Image, tweenInfo, goal2)
				tween2:Play()

				local tween3 = TweenService:Create(Title_2, tweenInfo, goal3)
				tween3:Play()

				local tween4 = TweenService:Create(Body, tweenInfo, goal3)
				tween4:Play()

				tween.Completed:Wait()
				Notification_2:Destroy()
			end
		end)()
	end,
}

